<!-- PLUGIN: Calendar -->
 <div class="widget_style">
            <div class="side_left_4">
                <div class="side_right_4">
                    <div class="side_top_4">
                        <div class="side_bot_4">
                            <div class="left_top_4">
                                <div class="right_top_4">
                                    <div class="left_bot_4">
                                        <div class="right_bot_4">
                                            <h2>Календарь</h2>
										</div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
			<center>
<table id="calendar" align="center">
<tr>
	<td class="month">[prev_link]&laquo;[/prev_link]</td>
	<td colspan="5" class="month">{current_link}</td>
	<td class="month">[next_link]&raquo;[/next_link]</td>
</tr>
<tr>
	<td class="weekday">{l_weekday_1}</td>
	<td class="weekday">{l_weekday_2}</td>
	<td class="weekday">{l_weekday_3}</td>
	<td class="weekday">{l_weekday_4}</td>
	<td class="weekday">{l_weekday_5}</td>
	<td class="weekend">{l_weekday_6}</td>
	<td class="weekend">{l_weekday_0}</td>
</tr>
{entries}
</table>
</center>

</div>
<!-- END plugin: Calendar -->
